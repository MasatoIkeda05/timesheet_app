<% provide(:title, "Log in") %>
<h1>Log in</h1>

<% flash.each do |key, value| %>
  <div>
    <%= value %>
  </div>
<% end %>

<%= form_for(:session, url: login_path) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Log in" %>
<% end %>



